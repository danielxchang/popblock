{
  "name": "Skipper",
  "description": "Skip ads and close popups.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/rabbit16.png",
      "32": "/images/rabbit32.png",
      "48": "/images/rabbit48.png",
      "128": "/images/rabbit128.png"
    }
  },
  "icons": {
    "16": "/images/rabbit16.png",
    "32": "/images/rabbit32.png",
    "48": "/images/rabbit48.png",
    "128": "/images/rabbit128.png"
  }
}
